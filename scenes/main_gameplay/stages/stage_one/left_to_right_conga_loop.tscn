script/source = "extends StagePhase

const ENEMY = preload(\"res://objects/enemy/enemy.tscn\")

@onready var curve = $Path2D.curve
var enemies: Array[Node] = []

func _ready():
	
	var points: Array[Vector2] = []
	for i in curve.point_count:
		points.push_back(curve.get_point_position(i))
	print(points)
	for i in 1:
		var e = ENEMY.instantiate()
		add_child(e)
		var c = EnemyController.new()
		e.add_child(c)
		c.position = points[0]
		enemies.append(e)
		behavior_move_to_points(c, points.slice(1), 0.5, 0.0, true)

func _process(delta: float) -> void:
	for e in enemies:
		if is_instance_valid(e):
			return
	phase_complete.emit()
	queue_free()
"

